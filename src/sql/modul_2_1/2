

CREATE TABLE IF NOT EXISTS employee
(
   ID     INT NOT NULL,
   SALARY INT DEFAULT NULL,
   PRIMARY KEY (ID)
);

INSERT INTO employee(ID, SALARY)
VALUES (1, 100),
      (2, 200),
      (3, 300);

SELECT MAX(SALARY) AS SecondHighestSalary
FROM employee
WHERE SALARY NOT IN (SELECT MAX(SALARY)
                     FROM employee);

SELECT MAX(SALARY) AS SecondHighestSalary
FROM employee
WHERE SALARY < (SELECT MAX(SALARY)
               FROM employee);
