

CREATE TABLE IF NOT EXISTS person
(
    ID    INT          NOT NULL,
    EMAIL VARCHAR(255) NOT NULL,
    PRIMARY KEY (ID)
);

INSERT INTO person(ID, EMAIL)
VALUES (1, 'a@b.com'),
            (2, 'c@d.com'),
            (3, 'a@b.com');


SELECT EMAIL
FROM person
GROUP BY EMAIL
HAVING COUNT(*) > 1;
